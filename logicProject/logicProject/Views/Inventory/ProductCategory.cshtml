@model logicProject.Models.EF.Product
@using logicProject.Models.EF

@{
    ViewBag.Title = "ProductCategory";
    var categories = (List<String>)ViewData["category"];
    //var products = (List<Product>)ViewData["products"];
    //Written By Antonio
    Layout = "~/Views/Inventory/_ClerkLayout.cshtml";
}

<h2>STATIONERY CATALOGUE</h2>
<p>Please select a category : </p>
<ul>
    @foreach (var ctgry in categories)
    {
        <li>@Html.ActionLink(ctgry, "ProductDetails", new { category = ctgry })</li>
    }

</ul>